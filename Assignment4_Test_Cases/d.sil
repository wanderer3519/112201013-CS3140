// Program to find the position of the first element in the array that doesn't equal to its index

decl
  integer arr[16];
  integer len;
  integer i;
enddecl

begin
  read(len); // Expecting 0 <= len <= 16
  
  for(i = 0; i < len; i++) {
    read(arr[i]);
  }

  for(i = 0; i < len; i++) {
    if(arr[i] != i) {
      break;
    }
  }

  write(i);
end
