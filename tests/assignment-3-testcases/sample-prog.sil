begindecl
    integer array[10];
    integer key, i;
    integer start, end, mid, result;
enddecl
for(i=0;i<10;i++){
    array[i] =i+10;
}
start=0;
end=9;
key=array[5]
for(; start<end;){
    mid=(start+end)/2
    if(array[mid]==key) { 
        result=mid; 
        break;
    }
    else if (array[mid]>key){ 
        start=mid+1;
    }
    else {
        end=mid-1;
    }
}
write(result);